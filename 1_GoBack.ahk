#Requires AutoHotkey v2.0

; ======================================
; Win+, → 뒤로 가기
; ======================================
#,:: 
{
    Send "^-"  ; Ctrl + - (뒤로 가기)
}

; ======================================
; Win+. → 앞으로 가기
; ======================================
#.:: 
{
    Send "^+-"  ; Ctrl + Shift + - (앞으로 가기)
}